                </xs:annotation>
            </xs:enumeration>

            <xs:enumeration value="A">
                <xs:annotation>
                    <xs:documentation xml:lang="fi">ammattistartti</xs:documentation>
                    <xs:documentation xml:lang="en">ammattistartti</xs:documentation>
                    <xs:documentation xml:lang="sv">yrkesstarten</xs:documentation>
                </xs:annotation>
            </xs:enumeration>

        </xs:restriction>
    </xs:simpleType>

</xs:schema>
