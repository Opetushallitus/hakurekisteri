!!! 5
html(ng-app="myApp")
  head
    title(oph-msg="suoritusrekisteri.title") Haku- ja valintarekisteri
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    link(type="text/css" href="static/css/bootstrap-3.3.1.min.css" rel="stylesheet")
    link(type="text/css" href="static/css/styles.css" rel="stylesheet")
    link(type="text/css" href="static/css/css-tyylikirjasto-virkailija.css" rel="stylesheet")
    link(type="text/css" href="static/css/sivumallit.css" rel="stylesheet")
    script(type="text/javascript" src="static/js/jquery-1.11.1.min.js")
    script(type="text/javascript" src="static/js/jquery.fileDownload.js")
    script(type="text/javascript" src="static/js/bootstrap-3.3.1.min.js")
    script(type="text/javascript" src="static/js/angular-1.3.2/angular.min.js")
    script(type="text/javascript" src="static/js/angular-1.3.2/angular-resource.min.js")
    script(type="text/javascript" src="static/js/angular-1.3.2/angular-route.min.js")
    script(type="text/javascript" src="static/js/angular-1.3.2/angular-sanitize.min.js")
    script(type="text/javascript" src="static/js/angular-locale/angular-locale_fi.js")
    script(type="text/javascript" src="static/js/angular-ui/ui-bootstrap-tpls-0.11.2.min.js")
    script(type="text/javascript" src="static/js/ng-upload.min.js")
    script(type="text/javascript" src="app.js")
    script(type="text/javascript" src="common-functions.js")
    script(type="text/javascript" src="controllers/murupolku-controller.js")
    script(type="text/javascript" src="controllers/opiskelijat-controller.js")
    script(type="text/javascript" src="controllers/muokkaa-controller.js")
    script(type="text/javascript" src="controllers/eihakeneet-controller.js")
    script(type="text/javascript" src="controllers/arvosana-controller.js")
    script(type="text/javascript" src="controllers/yoarvosana-controller.js")
    script(type="text/javascript" src="controllers/duplikaatti-controller.js")
    script(type="text/javascript" src="controllers/organisaatio-controller.js")
    script(type="text/javascript" src="controllers/tiedonsiirto-controller.js")
    script(type="text/javascript" src="controllers/hakeneet-controller.js")
    script(type="text/javascript" src="oph-msg-directive.js")
    script(type="text/javascript" src="configure.js")
    script(type="text/javascript" src="/virkailija-raamit/apply-raamit.js")

  body
    div(class="container")
      div(class="row" ng-controller="MurupolkuCtrl" ng-hide="isHidden()")
        div(class="col-xs-12")
          ul(class="breadcrumb")
            li
              a(href="#/")
                span(class="icon-breadcrumb-home")
            li(ng-repeat="muru in murupolku")
              a(ng-show="muru.href" href="{{muru.href}}")
                span(ng-show="muru.icon" class="{{muru.icon}}")
                span(oph-msg="{{muru.key}}") {{muru.text}}
              span(ng-hide="muru.href" oph-msg="{{muru.key}}") {{muru.text}}

      div(class="row")
        div(class="col-xs-12" ng-view)
